pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello I am in build stage'
            }
        }
        
         stage('Deploy') {
            steps {
                echo 'Hello I am in deploy stage'
            }
        }
        
         stage('Test') {
            steps {
                echo 'Hello I am in Test stage'
            }
        }
        
         stage('Release') {
            steps {
                echo 'Hello I am in Release stage'
            }
        }
    }
}

